{
  "name": "cryptobay",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "ENVFILE=.env react-native run-ios",
    "ios:storybook": "ENVFILE=.env.storybook react-native run-ios",
    "start": "react-native start",
    "test": "jest -u",
    "test:coverage": "jest -u --coverage --watchAll=false",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --fix '**/**/*.{ts,tsx,js}' 'App.tsx' 'storybook/**/*.{js,tsx}'",
    "format": "prettier --write 'src/**/*.{ts,tsx}' 'App.tsx' 'storybook/*.{js,tsx}' --config ./.prettierrc.js",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook",
    "prestorybook": "rnstl"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src/components"
      ],
      "pattern": "**/*.stories.tsx",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.0",
    "@testing-library/jest-native": "^4.0.12",
    "react": "18.1.0",
    "react-native": "0.70.1",
    "react-native-config": "^1.4.6",
    "react-native-reanimated": "^2.10.0",
    "react-native-safe-area-context": "^4.3.4",
    "react-native-screens": "^3.17.0",
    "react-native-storybook-loader": "^2.0.5",
    "react-native-svg": "^13.2.0",
    "react-navigation": "^4.4.4",
    "ts-jest": "^26.5.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@storybook/addon-actions": "^5.3",
    "@storybook/addon-knobs": "^5.3",
    "@storybook/addon-links": "^5.3",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.26",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@testing-library/jest-native": "^4.0.12",
    "@testing-library/react-native": "^11.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.70.0",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.5",
    "eslint": "^7.32.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.72.1",
    "react-dom": "18.1.0",
    "react-native-gesture-handler": "^2.6.2",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
